        "moment": "^2.29.1",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10",
        "vue2-flip-countdown": "^0.11.1"
    },
    "dependencies": {
        "@inertiajs/inertia": "^0.6.1",
        "@inertiajs/inertia-vue": "^0.4.1",
